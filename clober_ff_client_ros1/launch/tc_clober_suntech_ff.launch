<launch>
  <arg name="model" default="clober"/>
  <arg name="map_name" default="suntech_5x5"/>
  <arg name="map_file" default="$(find clober_navigation)/maps/$(arg map_name)/$(arg map_name).yaml"/>
  <arg name="open_rviz" default="true"/>
  <arg name="set_map_frame"  default="map"/>

  <arg name="cb_prefix" value="clober_0"/>

  <include file="$(find clober_bringup)/launch/tc_base.launch">
    <arg name="clober" value="$(arg cb_prefix)"/>
  </include>

  <group ns="$(arg cb_prefix)">

  <!-- Map server -->
  <node pkg="map_server" name="map_server" type="map_server" args="$(arg map_file)">
    <param name="frame_id" value="$(arg set_map_frame)"/>
  </node>

  <include file="$(find clober_ff_client_ros1)/launch/include/real_clober_cmb_ff.launch">
    <arg name="model" value="$(arg model)"/>
    <!-- <arg name="scan_topic" value="$(arg cb_prefix)/scan"/> -->
    <arg name="set_base_frame" value="$(arg cb_prefix)/base_footprint"/>
    <arg name="set_odom_frame" value="$(arg cb_prefix)/odom"/>
    <arg name="set_scan_frame" value="$(arg cb_prefix)/base_scan"/>
    <arg name="set_map_frame" value="$(arg set_map_frame)"/>
    <arg name="open_rviz" value="false"/>
    <arg name="initial_pose_x" default="1.8787"/>
    <arg name="initial_pose_y" default="0.3808"/>
    <arg name="initial_pose_a" default=" 0.0"/>
  </include>

  <node name="tc_agent" pkg="tc_agent" type="tc_agent_node" args="$(find tc_agent)/conf/$(arg map_name)/$(arg cb_prefix).json" output="screen"/>
  </group>

  <!-- launch the free fleet client of multi clobers -->
  <!-- <node name="$(arg cb_prefix)_free_fleet_client_node" pkg="free_fleet_client_ros1"
      type="free_fleet_client_ros1" output="screen">
    <param name="fleet_name" type="string" value="clober"/>
    <param name="robot_name" type="string" value="$(arg cb_prefix)"/>
    <param name="level_name" type="string" value="L1"/>
    <param name="dds_domain" type="int" value="123"/>
    <param name="max_dist_to_first_waypoint" type="double" value="10.0"/>
    <param name="battery_state_topic" value="/battery_state"/>
    <param name="map_frame" value="map"/>
    <param name="robot_frame" value="base_footprint"/>r
    <param name="move_base_server_name" value="move_base"/>
  </node> -->

  <!-- launch the overall visualization on rviz -->
  <!--<group ns="$(arg cb_prefix)"> 
  <node pkg="rviz" type="rviz" name="rviz" required="true" args="-d $(find clober_ff_client_ros1)/rviz/tc_clober_ff.rviz">
    <remap from="/initialpose" to="$(arg cb_prefix)/initialpose"/>
    <remap from="/move_base_simple/goal" to="$(arg cb_prefix)/move_base_simple/goal"/>
  </node>
 </group>-->
</launch>
